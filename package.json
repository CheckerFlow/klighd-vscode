{
  "name": "keith-richarts",
  "version": "0.1.0",
  "private": true,
  "electronVersion" : "2.0.4",
  "main": "src-gen/frontend/electron-main.js",

  "description": "Kiel Environment Integrated in Theia for Rich Charts",
  "theia": {
    "target": "electron"
  },
  "author": "Kiel University <rt-kieler-devel@informatik.uni-kiel.de>",
  "license": "EPL-1.0",
  "bugs": {
    "url": "https://rtsys.informatik.uni-kiel.de/jira"
  },
  "homepage": "https://rtsys.informatik.uni-kiel.de/kieler",
  "//": [
    "See why `yargs` is an explicit dependency: https://github.com/theia-ide/theia/issues/934",
    "See why `xterm` is an explicit dependency: https://github.com/theia-ide/theia/issues/737"
  ],
  "scripts": {
    "build": "theia build",
    "package": "yarn run build && electron-builder",
    "package:all": "yarn run build && electron-builder --linux --x64 && electron-builder --mac --x64",
    "package:all_old": "yarn run build && electron-builder --linux --x64 && electron-builder --mac --x64 && electron-builder --win --x64",
    "package:win": "yarn run build && electron-builder --win --x64",
    "package:linux": "yarn run build && electron-builder --linux --x64",
    "package:mac": "yarn run build && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac --x64",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "lerna run prepare",
    "rebuild:browser": "theia rebuild:browser",
    "rebuild:electron": "theia rebuild:electron",
    "publish:latest": "lerna publish --registry=https://registry.npmjs.org/ --exact --skip-git",
    "publish:next": "lerna publish --registry=https://registry.npmjs.org/ --exact --canary=next --npm-tag=next --skip-git --yes",
    "update": "git pull origin master && git submodule foreach git pull origin master"
  },
  "dependencies": {
    "@theia/core": "next",
    "@theia/editor": "next",
    "@theia/editorconfig": "next",
    "@theia/file-search": "next",
    "@theia/filesystem": "next",
    "@theia/languages": "next",
    "@theia/markers": "next",
    "@theia/messages": "next",
    "@theia/metrics": "next",
    "@theia/monaco": "next",
    "@theia/navigator": "next",
    "@theia/outline-view": "next",
    "@theia/preferences": "next",
    "@theia/preferences-api": "next",
    "@theia/preview": "next",
    "@theia/process": "next",
    "@theia/search-in-workspace": "next",
    "@theia/terminal": "next",
    "@theia/userstorage": "next",
    "@theia/variable-resolver": "next",
    "@theia/workspace": "next",
    "theia-sccharts-extension": "0.1.0",
    "yargs": "^9.0.1",
    "xterm": "^2.9.2",
    "vscode-jsonrpc": "^3.6.2",
    "vscode-nsfw": "^1.0.17",
    "@fortawesome/fontawesome-free" : "^5.2.0"
  },
  "devDependencies": {
    "@theia/cli": "next",
    "electron-builder": "^19.30.2",
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "0.0.31",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.3.5",
    "@types/temp": "^0.8.29",
    "@types/webdriverio": "^4.7.0",
    "lerna": "^2.4.0",
    "mocha": "^3.4.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "uuid": "^3.1.0",
    "vscode-jsonrpc": "^3.6.2",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^4.4.2",
    "webpack":"^4.16.1"
  },
  "workspaces": [
    "theia-sccharts-extension",
    "keith-app",
    "keith-app-electron"
  ]
}
