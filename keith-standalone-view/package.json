{
  "name": "@kieler/keith-standalone-view",
  "version": "0.1.0",
  "description": "Standalone web view for keith-sprotty diagrams",
  "author": "Kiel University <rt-kieler-devel@informatik.uni-kiel.de>",
  "license": "EPL-1.0",
  "keywords": [
    "sprotty",
    "keith",
    "diagram",
    "standalone"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "clean": "rimraf lib",
    "lint": "tslint -c ./tslint.json --project ./tsconfig.json",
    "build": "tsc",
    "watch": "tsc -w -p ./tsconfig.json",
    "start": "node ./lib/server.js",
    "socket": "node ./lib/server.js --LSP_PORT=5007"
  },
  "dependencies": {
    "@kieler/keith-sprotty": "0.1.0",
    "fastify": "^3.15.0",
    "fastify-static": "^4.0.1",
    "fastify-websocket": "^3.1.0",
    "pino-pretty": "^4.7.1",
    "vscode-ws-jsonrpc": "^0.2.0"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {}
}
