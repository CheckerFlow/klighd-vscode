{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es5",
    "outDir": "lib",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "sourceMap": true,
    "skipLibCheck": true
  }
}